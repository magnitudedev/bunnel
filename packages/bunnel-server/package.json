{
  "name": "bunnel-server",
  "version": "0.1.0",
  "description": "A simple HTTP tunnel server for local development",
  "type": "module",
  "main": "dist/server/server.js",
  "types": "dist/server/server.d.ts",
  "bin": {
    "bunnel-server": "dist/cli/server.js"
  },
  "files": [
    "dist/server",
    "dist/cli",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/server/server.ts src/cli/server.ts --outdir dist --target bun",
    "start": "bunnel-server",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tunnel",
    "http",
    "websocket",
    "development",
    "local",
    "proxy",
    "server"
  ],
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "commander": "^11.1.0",
    "selfsigned": "^2.4.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/commander": "^2.12.2",
    "esbuild": "^0.20.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "bun": "*"
  }
}
