{
  "name": "bunnel",
  "version": "0.1.4",
  "description": "Websocket reverse tunnel",
  "type": "module",
  "main": "dist/client/index.js",
  "types": "dist/client/index.d.ts",
  "bin": {
    "bunnel": "dist/cli/bunnel.js"
  },
  "files": [
    "dist/client",
    "dist/cli",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && pkgroll",
    "check": "tsc --noEmit",
    "start": "bunnel client",
    "prepare": "npm run build",
    "dev": "bun src/cli/bunnel.ts -l http://localhost:5173 -t http://localhost:4444"
  },
  "keywords": [
    "tunnel",
    "http",
    "websocket",
    "development",
    "local",
    "proxy"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magnitudedev/bunnel.git",
    "directory": "packages/bunnel"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^22.13.5",
    "@types/ws": "^8.18.0",
    "esbuild": "^0.20.1",
    "pkgroll": "^2.11.2",
    "typescript": "^5.0.0"
  }
}
